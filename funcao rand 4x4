// Hello world! Cplayground is an online sandbox that makes it easy to try out
// code.

#include <stdio.h>
#include <stdlib.h>

int main() {
    
    int matriz[4][4] = {
    {rand()%30, rand()%30, rand()%30, rand()%30},
    {rand()%30, rand()%30, rand()%30, rand()%30},
    {rand()%30, rand()%30, rand()%30, rand()%30},
    {rand()%30, rand()%30, rand()%30, rand()%30}
    };
     for(int i=0;i<4;i++){
        for(int j=0;j<4;j++){
            printf("%d\t", matriz[i][j]);
        }
        printf("\n");
     }
    return 0;
}
